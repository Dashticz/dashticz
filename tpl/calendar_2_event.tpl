<div class="event {{c1}} {{c2}} {{c3}} {{addClass}}" style="color:{{col}};" onclick="showInfo(this)"
     data-color="{{col}}" data-name="{{name}}" data-start="{{start}}" data-end="{{end}}"
          data-key="{{key}}" data-allday="{{allday}}" data-title="{{title}}" data-info="{{info}}" data-loc="{{loc}}">
  <div class="eventtime {{item.addClass}}">{{start}}</div>
  <div class="description {{item.addClass}}">{{title}}</div>
</div>